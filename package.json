{
  "name": "merkaba",
  "version": "0.0.0",
  "description": "An SVG editor component written in React",
  "main": "dist/app.js",
  "scripts": {
    "build": "webpack",
    "build:extras": "webpack --config webpack.extras.config.js",
    "start": "webpack-dev-server --config webpack.test.config.js",
    "test": "mocha -r jsdom-global/register ./node_modules/babel-core/register.js test/setup.js test/index.js",
    "test:watch": "nodemon --exec \"npm test\" --watch src --watch test",
    "test:browsers": "karma start",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm run deploy",
    "prettier": "prettier --single-quote --trailing-comma es5 '{src,test}/**/*.js' --write",
    "doc": "jsdoc -c .jsdoc src src/components",
    "doc:view": "live-server dist/doc --port=9124",
    "doc:watch": "nodemon --exec \"npm run doc\" --watch src --watch ./ --ext js,md --ignore dist",
    "doc:live": "concurrently --kill-others \"npm run doc:watch\" \"npm run doc:view\"",
    "deploy": "npm run build && npm run build:extras && gh-pages -d dist"
  },
  "author": "Jeremy Kahn <jeremyckahn@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@jeremyckahn/minami": "^1.3.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "karma": "^2.0.2",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ievms": "git+https://github.com/jeremyckahn/karma-ievms.git#browser-decorator",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.0",
    "live-server": "^1.2.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.12.1",
    "raf": "^3.4.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "compass-mixins": "^0.12.10",
    "react": "^16.3.2",
    "react-color": "^2.14.0",
    "react-dom": "^16.3.2",
    "react-draggable": "^3.0.5",
    "react-sortable-hoc": "^0.6.8",
    "react-tooltip": "^3.4.0",
    "transformation-matrix": "^1.10.0",
    "transformation-matrix-js": "^2.7.1"
  }
}
